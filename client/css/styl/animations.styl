@keyframes spin {
	from {
	    transform rotate(0deg)
	}
	to {
	    transform rotate(-360deg)
	}
}
@keyframes bird {
	from {
	    transform rotate(90deg)
	}
	to {
	    transform rotate(-90deg)
	}
}

@keyframes scale2 {
	from {
		width 100%
		height 95%
	}
	to {
		width 0%
		height 0%
	}
}


@keyframes bob {
	0% {
		transform translate3d(0px, -4px, 0px)
	}
	100% {
		transform translate3d(0px, 4px, 0px)
	}
}